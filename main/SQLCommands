Create Table LoginAttempts (
UserID varchar not null,
pictureset int not null,
loginmethod int not null,
selectedimageone varchar not null,
selectedimagetwo varchar not null,
selectedimagethree varchar not null,
timetakentochooseimageone float not null,
timetakentochooseimagetwo float not null,
timetakentochooseimagethree float not null,
overalltime float not null,
successfulimageone int not null,
	successfulimagetwo int not null,
	successfulimagethree int not null,
	overallsuccessful int not null,
	attemptnumber int not null,
	primary key (userid, pictureset, loginmethod, attemptnumber),
	foreign key (userid, pictureset, loginmethod) references userregistrations(userid, pictureset, loginmethod) on delete cascade on update cascade

)

