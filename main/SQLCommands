Create Table UserRegistrations (
UserID varchar not null,
PictureSet int not null,
LoginMethod int not null,
PasswordOne varchar not null,
PasswordTwo varchar not null,
PasswordThree varchar not null,
TimeTakenToChoosePasswordOne Float not null,
TimeTakenToChoosePasswordTwo Float not null,
TimeTakenToChoosePasswordThree Float not null,
TotalTimeTaken Float not null,
Primary Key (userID, pictureset, loginmethod)
)

Create Table MethodOneDecoyImageSet (
UserID varchar not null,
PictureSet int not null,
LoginMethod int not null,
ImageOne varchar not null,
ImageTwo varchar not null,
ImageThree varchar not null,
ImageFour varchar not null,
ImageFive varchar not null,
ImageSix varchar not null,
ImageSeven varchar not null,
	ImageEight varchar not null,
	ImageNine varchar not null,
	ImageTen varchar not null,
	ImageEleven varchar not null,
	ImageTwelve varchar not null,
	ImageThirteen varchar not null,
	ImageFourteen varchar not null,
	ImageFifteen varchar not null,
	ImageSixteen varchar not null,
	ImageSeventeen varchar not null,
	ImageEighteen varchar not null,
	ImageNineteen varchar not null,
	ImageTwenty varchar not null,
Primary Key (userID, pictureset, loginmethod),
Foreign Key (userID, PictureSet, loginmethod) references UserRegistrations (userid, pictureset, loginmethod) on delete cascade on update cascade
)

